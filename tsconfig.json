// prettier-ignore
{
  "extends": "@tsconfig/react-native/tsconfig.json",     /* Recommended React Native TSConfig base */
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig.json to read more about this file */

    /* Completeness */
    "noImplicitAny": true, 
    "skipLibCheck": true, 
    "baseUrl": "./src",
    "paths": {
      "@assets": [
        "assets"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@components": [
        "components"
      ],
      "@components/*": [
        "components/*"
      ],
      "@navigation" : [
        "navigation"
      ],
      "navigation/*" : [
        "navigation/*"
      ],
      "@pages": [
        "pages"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@utils": [
        "utils"
      ],
      "@utils/*": [
        "utils/*"
      ]
    }
  }
}
