// prettier-ignore
{
  "extends": "@tsconfig/react-native/tsconfig.json",     /* Recommended React Native TSConfig base */
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig.json to read more about this file */

    /* Completeness */
    "skipLibCheck": true, 
    "baseUrl": "./src",
    "paths": {
      "@assets": [
        "assets"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@constants/*" : [
        "constants/*"
      ],
      "@components": [
        "components"
      ],
      "@components/*": [
        "components/*"
      ],
      "@hooks": [
        "hooks"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@navigation" : [
        "navigation"
      ],
      "@navigation/*" : [
        "navigation/*"
      ],
      "@redux/*" : [
        "redux/*"
      ],
      "@test/*": [
        "test/*"
      ],
      "@ui/*": [
        "ui/*"
      ],
      "@utils": [
        "utils"
      ],
      "@utils/*": [
        "utils/*"
      ]
    }
  }
}
